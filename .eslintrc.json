{
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "plugin:cypress/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "@typescript-eslint",
        "cypress"
    ],
    "rules": {
        "@typescript-eslint/no-explicit-any": "warn",
        "cypress/no-unnecessary-waiting": "error"
    },
    "env": {
        "cypress/globals": true
    }
}